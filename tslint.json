{
  "defaultSeverity": "error",
  "extends": ["tslint-react", "tslint-config-prettier", "tslint-immutable"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": [true, ".prettierrc"],

    "interface-name": [true, "never-prefix"],
    "radix": true,
    "jsx-no-lambda": false,
    "prefer-object-spread": true
  }
}
