{
  "name": "mayoiga",
  "version": "0.1.5",
  "description": "Type safe hook based form library for React",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "/component.js",
    "/component.d.ts",
    "!*.test.js"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "test": "vitest run",
    "prebuild": "npm run clean",
    "build": "tsc",
    "check:type": "tsc --noEmit",
    "check:style": "prettier --check 'src/**/*.{js,ts,jsx,tsx}'",
    "check:lint": "eslint 'src/**/*.{ts,tsx}'",
    "check-all": "npm run check:type && npm run check:style && npm run check:lint && npm run build --noEmit && npm run test",
    "fix-all": "npm run check:style -- --write && npm run check:lint -- --fix",
    "prepublish": "npm run check-all && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/hachibeeDI/mayoiga.git"
  },
  "keywords": [
    "react",
    "react-dom",
    "forms",
    "validation",
    "state"
  ],
  "author": "OGURA Daiki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hachibeeDI/mayoiga/issues"
  },
  "homepage": "https://github.com/hachibeeDI/mayoiga#readme",
  "peerDependencies": {
    "zod": "^3.14.4"
  },
  "dependencies": {
    "nozuchi": "^0.0.3"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.19",
    "@types/use-sync-external-store": "^0.0.3",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-testing-library": "^5.6.3",
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typedoc": "^0.23.14",
    "typescript": "^4.8.3",
    "use-sync-external-store": "^1.2.0",
    "vitest": "^0.23.2",
    "zod": "^3.19.1"
  }
}
